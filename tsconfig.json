{
	"compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"es2016",
			"dom"
		],
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"suppressImplicitAnyIndexErrors": true,
		"baseUrl": "./",
		"paths": {
			"@/*":[
				"src/*"
			]
		}
	},
	"exclude": [
		"node_modules"
	]
}
